                $(document).ready(function() {

    $.datepicker.setDefaults({
        showOn: 'button',
        buttonImageOnly: true,
        dateFormat: 'mm/dd/yy',
	buttonImage: static_path + theme + "/img/calendar_icon.png",
        buttonText: 'Calendar'});

    $('#id_end_date').val("");
    $('#id_start_date').val("");

    initialize_dates()

    $('#id_start_date').datepicker();
    $('#id_end_date').datepicker();

    $("#id_calculation_form").validate({

        submitHandler: function(form) {
            if (check_date_order()) {
               form.submit();
            }
        },
        rules: {
            "employee_id": {
                digits: true,
                required: {}
            },
            "first_name": {
                required: {}
            },
            "last_name": {
                required: {}
            },
            "position_no": {
                required: {}
            },
            "position_title": {
                required: {}
            },
            "employee_type": {
                required: {}
            },
            "start_date": {
                required: {}
            },
            "end_date": {
                required: {}
            },
            "gl_account": {
                required: {}
            },
            "hourly_rate": {
                number: true,
                required: {}
            },
            "estimated_monthly_hours": {
                number: true,
                required: {}
            },
            "supervisor_name": {
                required: {}
            },
            "phone_extension": {
                required: {}
            }
        },
        messages: {}
    })

});

